DATA(lo_cols) = lo_alv->get_columns( ).
DATA lt_names TYPE STANDARD TABLE OF lvc_fname WITH EMPTY KEY.
DATA lo_col TYPE REF TO cl_salv_column_table.
DATA lv_name TYPE lvc_fname.

"Read all column names
LOOP AT lo_cols->get( ) INTO lo_col.
  TRY.
      lv_name = lo_col->get_columnname( ).
      INSERT lv_name INTO TABLE lt_names.
    CATCH cx_salv_not_found.
  ENDTRY.
ENDLOOP.

"Print list (will show in ABAP list)
LOOP AT lt_names INTO lv_name.
  WRITE: / lv_name.
ENDLOOP.