        TRY.
            lo_column ?= lo_columns->get_column( 'TDFORMAT' ).
            lo_column->set_short_text(  'Fmt Key' ).
            lo_column->set_medium_text( 'Format Key' ).
            lo_column->set_long_text(   'Format Key (TDFORMAT)' ).
          CATCH cx_salv_not_found.
        ENDTRY.